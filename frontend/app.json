{
  "expo": {
    "name": "El Rinc√≥n del Laurel",
    "slug": "rincon-laurel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rincon-laurel",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#6B5149"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rinconlaurel.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#6B5149"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rinconlaurel.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6B5149"
        }
      ]
    ],
    "extra": {
      "EXPO_PUBLIC_BACKEND_URL": "https://laurel-sync.preview.emergentagent.com"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
